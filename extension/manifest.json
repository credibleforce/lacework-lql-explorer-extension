{
  "manifest_version": 3,
  "name": "Lacework LQL Explorer",
  "description": "Explore Lacework LQL Datasources",
  "version": "0.0.4",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "host_permissions": [
    "*://*.lacework.net/*"
  ],
  "permissions": ["storage", "system.display"],
  "action": { 
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}